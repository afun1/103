{
  "version": 2,
  "functions": {
    "api/upload-vimeo.js": {
      "maxDuration": 30
    }
  },
  "builds": [
    {
      "src": "public/**/*",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/$1"
    },
    {
      "src": "/",
      "dest": "/public/index.html"
    },
    {
      "src": "/login",
      "dest": "/public/login.html"
    },
    {
      "src": "/signup",
      "dest": "/public/signup.html"
    },
    {
      "src": "/header",
      "dest": "/public/header.html"
    },
    {
      "src": "/(.*)",
      "dest": "/public/$1"
    }
  ]
}