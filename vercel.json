{
  "version": 2,
  "functions": {
    "api/upload-vimeo.js": {
      "maxDuration": 60
    },
    "api/search-customers.js": {
      "maxDuration": 30
    },
    "api/test-env.js": {
      "maxDuration": 10
    }
  },
  "builds": [
    {
      "src": "public/**/*",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/$1"
    },
    {
      "src": "/",
      "dest": "/public/index.html"
    },
    {
      "src": "/favicon.ico",
      "dest": "/public/supersparky.png"
    },
    {
      "src": "/supersparky.png",
      "dest": "/public/supersparky.png"
    },
    {
      "src": "/(.*)",
      "dest": "/public/$1"
    }
  ]
}